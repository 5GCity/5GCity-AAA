        "strip_context_path": false,
        "groups": [
            {
                "name": "default-group",
                "endpoints": [
                    {
                        "name": "default",
                        "target": "@APPCAT_IP@/",
                        "weight": 1,
                        "backup": false,
                        "type": "HTTP",
                        "http": {
                            "connectTimeout": 5000,
                            "idleTimeout": 60000,
                            "keepAlive": true,
                            "readTimeout": 10000,
